function void main() {
    graph initial = (graph) {1, 1};
    graph g = initial;
    node n1 = g.get(0);
    print(g.size());

    g = g + {2, 2};
    node n2 = g.get(1);
    print(g.size());

    g = g + {n1, n2};
    print(g.size());

    g = g - n1;
    print(g.size());

    print(initial == g);
}
